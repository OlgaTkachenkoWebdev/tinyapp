<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button
      class="navbar-toggler"
      type="button"
      data-toggle="collapse"
      data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup"
      aria-expanded="false"
      aria-label="Toggle navigation"
    >
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
        <a> 
            <% if(user['email']){ %>
              <a class="form-inline"> </a> <%= user['email'] %> </a>
              <form action="/logout" method="POST">
                <button type="submit" class="btn btn-primary">Log Out</button>
              </form>
            <% } else{ %>  
              <form class="form-inline" action="/login" method="POST">
                <div class="form-group mb-2">
                  <label for="email">Log in</label>
                  <input
                  class="form-control"
                  type="text"
                  name="emial"
                  placeholder="emial"
                  style="width: 100px; margin: 0em"
                  />
                  <button type="submit" class="btn btn-primary">Submit</button>
                </div>
              </form>
              <% } %>
        </a>
      </div>
    </div>
  </nav>
</header>